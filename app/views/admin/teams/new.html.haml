.page-header
  %h2 New Team
= simple_form_for @admin_team, url: { controller: 'teams', action: "create" }, html: {class: 'form-vertical'} do |f|

  .panel.panel-default
    .panel-body
      = render 'form', f: f
  .actions
    = button_tag type: 'submit', class: 'btn btn-success' do
      %span.glyphicon.glyphicon-edit
      Save
    = link_to 'Cancel', admin_teams_path, class: 'btn btn-default'

